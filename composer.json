{
    "name": "prepr/laravel-settings",
    "description": "Simple Settings package for laravel application for composer version 2",
    "keywords": [
    "Laravel",
    "Settings"
  ],
    "require": {
        "php": ">=7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Prepr\\LaravelSettings\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hemaro",
            "email": "hemaro@aistechnolabs.biz"
        }
    ],

  "extra": {
    "laravel": {
      "providers": [
        "PREPR\\Settings\\App\\Providers\\SettingServiceProvider"
      ],
      "aliases": {
        "Settings": "PREPR\\Settings\\App\\Facades\\Setting"
      }
    }
  },
    "minimum-stability": "stable"
}

{
    "name": "prepr2/laravel-settings",
    "description": "Simple Settings package for laravel application for composer version 2",
    "keywords": [
        "Laravel",
        "Settings"
    ],
    "require": {
        "php": ">=7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Prepr2\\Settings\\": "src/"
        }
    },
    "authors": [
        {
            "name": "hemaro",
            "email": "hemaro@aistechnolabs.biz"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "PREPR2\\Settings\\App\\Providers\\SettingServiceProvider"
            ],
            "aliases": {
                "Settings": "PREPR2\\Settings\\App\\Facades\\Setting"
            }
        }
    }
}
